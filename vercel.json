{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/reg", "dest": "/api/reg.php" },
    { "src": "/postReg", "dest": "/api/postReg.php" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}